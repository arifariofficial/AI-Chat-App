{
  "name": "sipe-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:dev": "dotenv -e .env.local -- next build",
    "start": "next start",
    "start:dev": "dotenv -e .env.local -- next start",
    "lint": "next lint",
    "test": "jest",
    "cypress:open": "cypress open",
    "analyze": "ANALYZE=true next build",
    "scrape": "tsx scripts/scrape.ts",
    "embed": "tsx scripts/embed.ts",
    "docker:jenkins": "docker build --platform linux/amd64 -f jenkins.Dockerfile -t arifariofficial/sipe-jenkins . && docker push arifariofficial/sipe-jenkins:latest",
    "docker:dev": "docker build -f Dockerfile -t arifariofficial/sipe-dev . && docker push arifariofficial/sipe-dev:latest",
    "docker:push": "docker build --platform linux/amd64 -t arifariofficial/sipe . --no-cache && docker push arifariofficial/sipe:latest",
    "docker:build": "docker-compose -f docker-compose.dev.yml build",
    "docker:up": "docker-compose -f docker-compose.dev.yml up",
    "docker:prune": "docker system prune -f && docker image prune -f",
    "migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "generate": "dotenv -e .env.development -- npx prisma generate",
    "prisma": "dotenv -e .env.development -- npx prisma studio",
    "seed": "dotenv -e .env.development tsx prisma/seed.ts"
  },
